{
  "name": "koa-decorator-ts",
  "version": "1.3.3",
  "description": "koa decorator",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "tsc",
    "test": "cross-env NODE_ENV=test jest  --no-cache --forceExit --runInBand"
  },
  "author": "skmdev29 <skmdev29@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skmdev/koa-decorator-ts.git"
  },
  "license": "ISC",
  "dependencies": {
    "glob": "^7.1.2",
    "koa": "^2.5.1",
    "koa-compose": "^4.0.0",
    "koa-jwt": "^3.3.1",
    "koa-router": "^7.4.0"
  },
  "devDependencies": {
    "@types/glob": "^5.0.35",
    "@types/jest": "^22.2.3",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/koa": "^2.0.45",
    "@types/koa-bodyparser": "^4.2.0",
    "@types/koa-router": "^7.0.28",
    "@types/node": "^9.6.7",
    "@types/supertest": "^2.0.4",
    "apollo-server-koa": "^1.3.6",
    "cross-env": "^5.1.4",
    "graphql": "^0.13.2",
    "graphql-import": "^0.5.2",
    "graphql-tools": "^2.24.0",
    "jest": "^22.4.3",
    "jsonwebtoken": "^8.2.1",
    "koa-bodyparser": "^4.2.0",
    "koa-graphql": "^0.7.5",
    "supertest": "^3.0.0",
    "ts-node": "^6.0.1",
    "typescript": "^2.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "verbose": true,
    "coverageDirectory": "coverage",
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "text"
    ]
  },
  "bugs": {
    "url": "https://github.com/skmdev/koa-decorator-ts/issues"
  },
  "homepage": "https://github.com/skmdev/koa-decorator-ts#readme"
}
